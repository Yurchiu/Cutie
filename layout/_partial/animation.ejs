<% if(theme.colored_eggs.type == "santa"){ %>

<a class="santa-container" data-bs-toggle="tooltip" title="Don't touch me! qwq">
  <div class="santa" onclick="santaColorEgg()">
    <div class="body" style="font-size: <%- theme.colored_eggs.width %>">
        <div class="head">
            <div class="ear"></div>
            <div class="ear"></div>
            <div class="hat"></div>
            <div class="eye"></div>
            <div class="eye"></div>
            <div class="nose"></div>
            <div class="mouth"></div>
        </div>
    </div>
  </div>
</a>

<% }else if(theme.colored_eggs.type == "natsuki"){%>

<div class="natsuki-container" style="width: <%- theme.colored_eggs.width %>" data-bs-toggle="tooltip" title="Don't touch me! qwq">
    <img id="natsuki2" width="100%" height="100%" src="<%- theme.in_cdn %>/img/natsuki2.png">
    <img id="natsuki1" width="100%" height="100%" src="<%- theme.in_cdn %>/img/natsuki1.png">
</div>
<script type="text/javascript">
    $("#natsuki1").click(function(){
        $("#natsuki1").hide();
        $("#natsuki2").show();
        $("#natsuki2").css("animation","natsuki <%- theme.colored_eggs.time %> <%- theme.colored_eggs.bezier %>");
    });
    $("#natsuki2").each(function(){
        $(this)[0].addEventListener("animationend",function() {
        $("#natsuki2").hide();
        $("#natsuki1").show();
        $(this).css("animation","");
        });
    });
</script>

<% } %>